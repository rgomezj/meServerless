{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "Index.html": {
      "matchCondition": {
        "route": "/"
      },
      "backendUri": "https://rogergomez.z13.web.core.windows.net/Admin"
    },
    "CSS Passthrough": {
      "matchCondition": {
        "route": "/css/{*restOfPath}"
      },
      "backendUri": "https://rogergomez.z13.web.core.windows.net/Admin/css"
    },
    "Javascript Passthrough": {
      "matchCondition": {
        "route": "/js/{*restOfPath}"
      },
      "backendUri": "https://rogergomez.z13.web.core.windows.net/Admin/js"
    },
    "Template Passthrough": {
      "matchCondition": {
        "route": "/template/{*restOfPath}"
      },
      "backendUri": "https://rogergomez.z13.web.core.windows.net/Admin/template"
    }
  }
}